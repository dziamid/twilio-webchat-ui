  },
  "bundledDependencies": ["@twilio/frame-ui"],
  "devDependencies": {
    "@types/chai": "^4.0.4",
    "@types/jest": "^22.2.3",
    "@types/react": "^16.4.6",
    "@types/react-dom": "^16.0.6",
    "chai": "^4.1.2",
    "child_process": "^1.0.2",
    "del": "^3.0.0",
    "fancy-log": "^1.3.2",
    "file-loader": "^0.11.1",
    "gulp": "^3.9.1",
