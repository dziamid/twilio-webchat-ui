    "emotion": "9.2.6",
    "emotion-theming": "9.2.6",
    "lodash": "^4.17.5",
    "loglevel": "^1.6.1",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-draggable": "^3.0.5",
