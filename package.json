{
  "name": "@twilio/flex-webchat-ui",
  "private": false,
  "version": "1.0.0-dz",
  "description": "Twilio Customer Frame",
  "main": "bundle/twilio-flex-webchat.prod.js",
  "types": "src/index.d.ts",
  "scripts": {
    "gulp": "./node_modules/gulp/bin/gulp.js",
    "test": "./node_modules/gulp/bin/gulp.js runUnitTest",
    "start": "webpack-dev-server --hot --env=dev.browser --content-base=resources --port=8081",
    "start:https": "webpack-dev-server --hot --https --env=dev.local --content-base=resources --port=8081",
    "build:dev": "webpack --env=dev --progress --colors",
    "build:dev.browser": "webpack --env=dev.browser --progress --colors",
    "build:prod": "webpack --env=prod --progress --colors",
    "build:prod.browser": "webpack --env=prod.browser --progress --colors",
    "doc": "./node_modules/gulp/bin/gulp.js doc",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "requirements-check": "node check-version.js",
    "postinstall": "npm run requirements-check"
  },
  "author": "Twilio",
  "license": "Apache-2.0",
  "dependencies": {
    "@material-ui/core": "^1.3.1",
