    "ts-loader": "^4.4.1",
    "ts-mockito": "^2.1.1",
    "ts-node": "^3.0.6",
    "tsconfig-paths-webpack-plugin": "^3.0.3",
    "tslint": "^5.7.0",
    "typescript": "3.0.1",
    "url-loader": "^0.5.8",
